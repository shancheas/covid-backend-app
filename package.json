{
  "name": "eigen",
  "version": "1.0.0",
  "description": "service for handle persistent data",
  "main": "app.js",
  "author": "Irfan",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc",
    "dev": "tsc-watch --onSuccess \"node -r dotenv/config ./dist/index.js\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --config ./.prettierrc",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" --config ./.prettierrc",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --fix",
    "test": "jest"
  },
  "dependencies": {
    "@types/jest": "^26.0.24",
    "eslint": "^7.12.1",
    "express": "^4.17.1",
    "jest": "^27.0.6",
    "prettier": "^2.1.2",
    "redis": "^3.1.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/mime-types": "^2.1.0",
    "@types/redis": "^2.8.31",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
